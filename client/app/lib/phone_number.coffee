CODES = db = {}

module.exports = normalizeNumber = (value, defaultcode) ->
    number = value.replace /[- \+]/g, ''
    if number[0] isnt '0' then number
    else if number[1] is 0
        number.substring 2 # 00XX = +XX
    else
        defaultcode + number.substring 1

