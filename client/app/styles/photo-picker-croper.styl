#objet-picker

  .photoPicker
    display:flex

  .thumbPicker
    overflow:auto

  .fp-nav-tabs
    background-color:#ddd
    padding-top:40px
    margin-right:15px
    flex-shrink:0
    // border-color:#ddd
    // border-width:15px

  [role='tab']
    padding-right:8px
    padding-left:8px
    width:50px
    height:35px
    font-weight:bold
    color:#3F3F3F
    cursor:pointer
    text-align: center
    line-height: 35px

  [aria-selected='true']
    background-color:#fff

  [aria-hidden="true"]
    display:none

  .thumbsContainer
    img
      border: 5px solid transparent
      height: 120px
      margin-bottom: 2.5px
      margin-right: 2.5px
      cursor:pointer
    img.selected
      border: 5px solid #34a6ff
      border-radius: 3px
      -webkit-border-radius: 3px
      -ms-filter: none
      filter: none

  // [role='tabpanel']
